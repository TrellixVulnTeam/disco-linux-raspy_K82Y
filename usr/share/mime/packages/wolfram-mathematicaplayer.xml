<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/mathematicaplayer">
    <comment>Mathematica Player Notebook</comment>
    <magic priority="80">
    	<match type="string" offset="0:100" value="application/mathematica" />
    </magic> 
    <glob pattern="*.nbp"/>
  </mime-type>
</mime-info>
