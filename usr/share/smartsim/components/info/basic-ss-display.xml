	<bound right="20" down="35" left="-20" up="-35"/>
	<pin x="15" y="35" direction="down" length="10" flow="in" required="false"/>
	<pin x="5" y="35" direction="down" length="10" flow="in" required="false"/>
	<pin x="-5" y="35" direction="down" length="10" flow="in" required="false"/>
	<pin x="-15" y="35" direction="down" length="10" flow="in" required="false"/>
	<pin x="25" y="35" direction="down" length="10" flow="in" required="false" show="false"/>
</component>
