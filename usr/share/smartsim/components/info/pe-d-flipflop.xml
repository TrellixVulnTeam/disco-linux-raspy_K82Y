<?xml version="1.0" encoding="UTF-8"?>
<component>
	<name>PE D Flipflop</name>
	<description>Positive-edge triggered D flipflop. Stores a binary bit which is set to "D" as the clock becomes 1.</description>
	<label>DFF</label>
	<icon>pe-d-flipflop.png</icon>
	<bound right="20" down="30" left="-20" up="-30"/>
	<pin x="-20" y="-20" direction="left" length="10" flow="in" label="D"/>
	<pin x="-20" y="20" direction="left" length="10" flow="in" symlabel="clock"/>
	<pin x="20" y="-20" direction="right" length="10" flow="out" label="Q" required="false"/>
	<pin x="20" y="20" direction="right" length="10" flow="out" barlabel="Q" required="false"/>
</component>
